age=25
array = [273, 23.3, '안녕하세요', True, age]
print(array)
array_2 = [100, array]

print(array_2)
# index of `array` in `array_2`
print(array_2[1][2])



temp = [
    [1,2],   #temp[0] --- row0
    [10,20], #temp[1]   temp[1][1]
    [30,40]  #temp[2]
]

print(temp[2])
print(temp[0][0])


